<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.beta.dataface">

    <uses-sdk android:minSdkVersion="11" android:targetSdkVersion="16"/>

    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.beta.dataface.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_list_handler" android:name="com.beta.dataface.ListHandlerActivity" android:parentActivityName="com.beta.dataface.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="com.beta.dataface.MainActivity"/>
        </activity>
        <activity android:label="@string/title_activity_add_new_object" android:name="com.beta.dataface.AddNewObject" android:parentActivityName="com.beta.dataface.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="com.beta.dataface.MainActivity"/>
        </activity>
        <activity android:label="@string/title_activity_list_child" android:name="com.beta.dataface.ListChildActivity" android:parentActivityName="com.beta.dataface.ListHandlerActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="com.beta.dataface.ListHandlerActivity"/>
        </activity>
    </application>

</manifest>
